{
  "mongo": {
    "onlyUsePrimary": true,
    "connection": {
      "hosts": [
        "registry.gitlab.com-gitlab-org-gitter-webapp-mongo:27017"
      ],
      "database": "gitter",
      "options": {
      }
    },
    "profileSlowQueries": true,
    "logQueries": false,
    "noAutoIndex": true
  },
  "logging": {
    "logToFile": false,
    "level": "error",
    "disableConsole": true,
    "colorize": false,
    "prettyPrint": true,
    "access": false,
    "loggingFormat": "dev",
    "logStaticAccess": false,
    "logEmailContents": false
  },
  "presence": {
    "prefix": "tpr"
  },
  "irc": {
    "clientKey": "irc-dev"
  },
  "redis": "redis://redis?db=1",
  "redis_nopersist": "redis://redis?db=1",
  "redis_caching": "redis://redis",
  "elasticsearch": {
    "comment": "Override with GITTER_ES_ENDPOINT (comma separated)",
    "hosts": [
      "http://registry.gitlab.com-gitlab-org-gitter-webapp-elasticsearch:9200"
    ]
  },
  "test": {
    "exposeInBrowserTests": true,
    "exposeDataForTestingPurposes": true
  },
  "gitlaboauth": {
    "consumer_key": "",
    "consumer_secret": ""
  },
  "twitteroauth": {
    "consumer_key": "",
    "consumer_secret": ""
  },
  "linkedinoauth2": {
    "client_id": "",
    "client_secret": ""
  },
  "googleoauth2": {
    "client_id": "",
    "client_secret": ""
  },
  "github": {
    "client_id": "",
    "client_secret": "",
    "user_client_id": "",
    "user_client_secret": "",
    "caching": true,
    "fast-repo-description-cache-timeout": 60,
    "anonymous_app": {
      "client_id": "",
      "client_secret": ""
    },
    "foreverAgent": true
  },
  "neo4j": {
    "comment": "Override with GITTER_NEO4J_ENDPOINT",
    "endpoint": "http://neo4j:7474"
  },
  "resque": {
    "namespace": "resque-test",
    "queuePrefix": "test"
  },
  "transloadit": {
    "bucket": "",
    "key": "",
    "secret": "",
    "template_id": "",
    "template_image_id": "",
    "template_avatar_id": "",
    "cname": "",
    "avatars": {
      "bucket": ""
    }
  }
}
