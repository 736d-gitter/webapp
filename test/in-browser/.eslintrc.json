{
  "env": {
    "commonjs": true,
    "browser": true,
    "mocha": true
  },
  "plugins": [
    "mocha"
  ],
  "rules": {
    "mocha/no-exclusive-tests": "error",
    "strict": [
      "warn",
      "safe"
    ],
    "max-nested-callbacks": [
      "error",
      10
    ],
    "node/no-unpublished-require": "off",
    "node/no-missing-require": "off"
  }
}
