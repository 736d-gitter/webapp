<div class="whicon">
  <img class="notification-icon" src="{{favicon}}"> 
</div>

<div class="whbody">
  <a href="https://github.com/{{user}}" target="_blank">{{user}}</a> {{human_action}} <a class="toggleCommits">{{commits.length}} commit(s)</a> <a href="https://github.com/{{repo}}" target="_blank">{{repo}}</a> to {{branch}}

  <ul class="commits">
  {{#each commits}}
  <li><a href="{{url}}" target="_blank">{{message}}</a></li>
  {{/each}}
  </ul>
</div>
