<aside id="room-menu-container" class="room-menu-container">
  <section class="room-menu">
    <nav id="minibar" class="minibar">
      <ul class="room-menu-options">

        <li data-state-change="all" class="room-menu-options__item--home">
          <a><img class="room-menu-options__item__avatar" src='{{cdn "images/gitter-vector-logo.svg"}}'/></a>
        </li>

        <li data-state-change="search" class="room-menu-options__item--search">
          <a><img class="room-menu-options__item__avatar" src='{{cdn "images/svg/menu/search.svg"}}'/></a>
        </li>

        <li data-state-change="favourite" class="room-menu-options__item--favourite">
          <a><img class="room-menu-options__item__avatar" src='{{cdn "images/svg/menu/favourites.svg"}}'/></a>
        </li>

        <li data-state-change="people" class="room-menu-options__item--people">
          <a><img class="room-menu-options__item__avatar" src='{{cdn "images/svg/menu/people.svg"}}'/></a>
        </li>

        {{#each orgs}}
          {{! JP 9/12/15 }}
          {{! TODO SHOULD THIS BE MOVED INTO A PARTIAL TO BE SHARED AROUND }}
          <li
            data-state-change="org"
            data-org-name="{{name}}"
            class="room-menu-options__item--org" >
            <a><img class="room-menu-options__item__avatar" src="{{imgUrl}}"  /></a>
          </li>
        {{/each}}

      </ul>
    </nav>
    <div id="room-menu__panel">
      <section class="room-menu__panel">
        <header class="panel-header" id="panel-header"></header>
        <div id="profile-menu"></div>
        <div class="nano">
          <div class="nano-content">
            <div id="search-input"></div>
            {{!<div id="search-results">i }}
              {{! {{{ prerenderView "js/views/menu/room/search-results/search-results-view" wrap="div" }}
            {{! </div> }}

            <section class="primary-collection" id="primary-collection"></section>
            <section class="secondary-collection" id="secondary-collection"></section>
          </div>
        </div>
        <footer id="panel-footer">This is working</footer>
      </section>
    </div>
  </section>
</aside>
