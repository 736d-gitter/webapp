<div class="gtrArchiveNavigationInner">
  <div class="gtrArchiveDate">
    {{#if nextDate}}<a href="{{nextDateLink}}"><i class="gtrArchiveNext octicon octicon-chevron-right"></i></a>{{/if}}
    {{#if previousDate}}<a href="{{previousDateLink}}"><i class="gtrArchivePrev octicon octicon-chevron-left"></i></a>{{/if}}
    <div class="gtrArchiveToday">
      <span class="gtrArchiveDay">{{dayName}}<sup>{{dayOrdinal}}</sup></span><br>
      <span class="gtrArchiveMonth">{{monthYearFormatted}}</span>
    </div>
  </div>


  <div id="archive-navigation" class="gtrArchiveCalendar">
  </div>
</div>
