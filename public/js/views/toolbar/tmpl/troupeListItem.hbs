{{#if ethereal}}
  {{#if createRoom}}
      <span class="github-REPO">{{name}}</span>
  {{else}}
    <span class="github-REPO disabled">{{name}}</span>
  {{/if}}
{{else}}
    <span class="github-{{githubType}}">{{name}}</span>
    {{#if unreadItems}}
    <span class="item-unread-badge">
      <span class="trpTroupeUnreadCounter">{{unreadItems}}</span>
    </span>
    {{/if}}
{{/if}}
