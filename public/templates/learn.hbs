    <section class="what">
      <h3>Gitter API</h3>
      <a class="section-link" href="https://developer.gitter.im">developer.gitter.im</a>
      <a class="other-link" href="https://developer.gitter.im/docs/rest-api">Rest API</a><br>
      <a class="other-link" href="https://developer.gitter.im/docs/streaming-api">Streaming API</a><br>
      <a class="other-link" href="https://developer.gitter.im/docs/faye-endpoint">Faye</a><br>
      <a class="other-link" href="https://developer.gitter.im/docs/services">Services</a>
    </section>
    <section class="what">
      <h3>On Gitter</h3>
      <p><a href="/gitterHQ/gitter" target="_parent">gitterHQ/gitter</a> for general product stuff</p>
      <p><a href="/gitterHQ/developers" target="_parent">gitterHQ/developers</a> for Gitter API developer questions</p>
    </section>
    <section class="what">
      <h3>Old School</h3>
      <a class="section-link" target="_parent" href="mailto:team@gitter.im">team@gitter.im</a>
    </section>
    <section class="what">
      <h3>Twitter</h3>
      <a class="section-link" href="https://twitter.com/gitchat">@gitchat</a>
    </section>
    <section class="what">
      <h3>Raise an Issue</h3>
      <a class="section-link" href="https://github.com/gitterHQ/gitter/issues">github.com/gitterHQ/gitter/issues</a>
    </section>
  </div>

  {{{ generateTroupeContext troupeContext }}}
