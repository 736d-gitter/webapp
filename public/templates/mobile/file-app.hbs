<!doctype html>
<html lang="en" {{#if appCache}}manifest="{{cdn appCache notStatic=true }}"{{/if}}>
<head>

  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1"/>
  <meta http-equiv="X-UA-Compatible" content="chrome=1">
	<meta charset="utf-8">

	<title>{{troupeName}} - Troupe</title>

  <link rel="stylesheet" href='/bootstrap/css/trpMobileFiles.css'>

  <script type="text/javascript">
  window.troupeContext = {{{troupeContext}}};
  {{#if data}}
  window.troupePreloads = {{{data}}};
  {{/if}}
  </script>

  {{>require_wrapper}}

  {{{bootScript "routers/mobile/native/files-router" skipCdn=true }}}

    <!-- Le fav and touch icons -->
    <link rel="shortcut icon" href='{{ cdn "images/2/favicon8.ico"}}'>

</head>
<body>

  <div class="trpMobileAmuse fade-in" id="file-amuse">
    <div class="trpMobileAmuseIcon">
      <img src='{{cdn "images/2/troupe-ajax-guy-green.gif" }}'><br><br>Loading
    </div>
  </div>

  <div class="contentBody mobile-scroll mobile-scroll-class" id="file-wrapper">
    <div id="primary-view">
      <!-- the chat content below is an ugly ugly hack, will sort it out. Fluid rows have their downside. -->
      <div class="frame-files">
      </div>
    </div>
  </div>
</body>
</html>
