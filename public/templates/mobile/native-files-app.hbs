<!doctype html>
<html lang="en" {{#if appCache}}manifest="{{appCache}}"{{/if}}>
<head>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1"/>
  <meta http-equiv="X-UA-Compatible" content="chrome=1">
	<meta charset="utf-8">

	<title>Troupe</title>

  <link rel="stylesheet" href='{{ cdn "bootstrap/css/trpMobileFiles.css" appcache=appCache }}'>

  {{{ generateTroupeContext troupeContext accessToken=accessToken page='files' appcache=appCache }}}

    <link rel="shortcut icon" href='{{ cdn "images/2/favicon8.ico"  appcache=appCache }}'>

</head>
<body>

  <div class="trpMobileAmuse fade-in" id="file-amuse">
    <div class="trpMobileAmuseIcon">
      <img src='{{cdn "images/2/troupe-ajax-guy-green.gif"  appcache=appCache }}'><br><br>Loading
    </div>
  </div>

  <div class="contentBody mobile-scroll mobile-scroll-class" id="file-wrapper">
    <div id="primary-view">
      <!-- the chat content below is an ugly ugly hack, will sort it out. Fluid rows have their downside. -->
      <div class="frame-files">
      </div>
    </div>
  </div>

  {{>require_wrapper}}
  {{{bootScript "routers/mobile/native/files-router" appcache=appCache }}}

</body>
</html>
