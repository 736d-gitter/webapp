<script type="text/javascript" async defer>

  function loadCss(url) {
    var link = document.createElement("link");
    link.type = "text/css";
    link.rel = "stylesheet";
    link.href = url;
    document.getElementsByTagName("head")[0].appendChild(link);
  }

  loadCss('//fonts.googleapis.com/css?family=Open+Sans:400,300,700&subset=latin,cyrillic,vietnamese');
  // loadCss('//cdnjs.cloudflare.com/ajax/libs/octicons/2.1.2/octicons.min.css');

</script>
