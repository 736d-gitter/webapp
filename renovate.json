{
  "platform": "gitlab",
  "endpoint": "https://gitlab.com/api/v4/",
  "repositories": ["gitlab-org/gitter/webapp"],

  "logLevel": "info",

  "requireConfig": true,
  "onboarding": true,
  "onboardingConfig": {
    "extends": ["config:base"],
    "prConcurrentLimit": 5
  },

  "enabledManagers": ["npm"],
  "ignorePaths": ["modules/**/*"]
}
